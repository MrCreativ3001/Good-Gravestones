{
  "schemaVersion": 1,
  "id": "goodgravestones",
  "version": "${version}",
  "name": "Good Gravestones",
  "description": "This mod adds a gravestones after a death.",
  "authors": [
    "MrCreativ3001"
  ],
  "contact": {
    "homepage": "https://example.com/",
    "issues": "https://example.com/",
    "sources": "https://example.com/"
  },
  "license": "MIT License",
  "icon": "assets/goodgravestones/logo.jpg",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "de.mrcreativ3001.goodgravestones.SimpleGravestones"
      }
    ],
    "modmenu": [
      {
        "adapter": "kotlin",
        "value": "de.mrcreativ3001.goodgravestones.config.SimpleGravestonesModMenuApi"
      }
    ]
  },
  "mixins": [
    "goodgravestones.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabricloader}",
    "fabric-api": ">=${fabric_api}",
    "fabric-language-kotlin": ">=${fabric_language_kotlin}",
    "minecraft": ">=${minecraft}",
    "java": ">=${java}"
  }
}